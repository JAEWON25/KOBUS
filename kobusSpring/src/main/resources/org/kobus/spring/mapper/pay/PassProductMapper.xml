<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.kobus.spring.mapper.pay.PassProductMapper">

    <select id="getPriceByProductSno" resultType="int">
        SELECT PRICE
        FROM SEASON_TICKET_OPTION
        WHERE ADTN_PRD_SNO = #{prdSno}
    </select>

</mapper>
